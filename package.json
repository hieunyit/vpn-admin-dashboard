{
  "name": "vpn-admin-dashboard",
  "version": "1.0.0",
  "description": "VPN Management Dashboard with Vue 3",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.45.0",
    "eslint-plugin-vue": "^9.15.1",
    "postcss": "^8.5.5",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.4.17",
    "vite": "^4.4.5",
    "vitest": "^0.34.1",
    "vue-tsc": "^1.8.5"
  },
  "keywords": [
    "vue3",
    "vpn",
    "admin",
    "dashboard",
    "management",
    "pinia",
    "tailwindcss",
    "vite"
  ],
  "author": "VPN Admin Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
